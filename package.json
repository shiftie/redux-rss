{
  "name": "react-starter-es6-babel",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "ecstatic": "~0.8.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  }
}
